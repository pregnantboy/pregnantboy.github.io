{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["off", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"no-undef": "off",
		"no-console": "off",
		"function-paren-newline": ["error", "never"],
		"prefer-arrow-callback": "error",
		"array-bracket-newline": ["error", "never"],
		"array-element-newline": ["error", { "minItems": 3 }]
	}
}
