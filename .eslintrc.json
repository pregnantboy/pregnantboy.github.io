{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"no-undef": "off",
		"no-console": "off",
		"array-bracket-newline": [
			"error",
			{
				"multiline": true,
				"minItems": 1
			}
		],
		"function-paren-newline": ["error", "never"],
		"prefer-arrow-callback": "error"
	}
}
